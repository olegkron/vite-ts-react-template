    "jsdom": "^24.0.0",
    "postcss": "^8.4.33",
    "precss": "^4.0.0",
    "prettier": "^3.2.4",
    "stylelint": "^16.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "vite": "^5.0.12",
    "vite-plugin-environment": "^1.1.3",
